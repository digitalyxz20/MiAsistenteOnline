@model IEnumerable<MiAsistenteOnline.Web.Data.Entities.Product>

@{
    ViewData["Title"] = "ProductosPorCategoria";
}

<h2>ProductosPorCategoria</h2>

<a asp-action="Categoria">Regresar</a>

<div class="row row-cols-1 row-cols-md-3">
    @foreach (var item in Model)
    {



        <div class="col mb-4">
            <div class="card">
                <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
                <div class="card-body">
                    <center><h5 class="card-title">@Html.DisplayFor(modelItem => item.Name)</h5></center>

                </div>
                <center><p class="card-text"><h3>S/@Html.DisplayFor(modelItem => item.Price)</h3></p></center>
                <center ><p class="card-text"><input size="3" border="5" value="1" placeholder="Cantidad"/></center>
                <center><p class="card-text"><button type="button" class="btn btn-success" style="margin:5%" >Añadir</button></center>
            </div>

        </div>


    }
 </div>
